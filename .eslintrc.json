{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true,
        "jquery": true
    },
    "extends": [
        "airbnb-typescript"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "project": "./tsconfig.json"
     },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "@typescript-eslint/indent": "off"
    }
}
